{
  "src": "./src",
  "language": "typescript",
  "schema": "./src/api/schema.graphql",
  "exclude": [
    "**/node_modules/**",
    "**/__mocks__/**",
    "**/__generated__/**"
  ],
  "eagerEsModules": true,
  "customScalarTypes": {
    "DateTime": "string",
    "Jwt": "string",
    "Semver": "string",
    "UnsignedInt": "number",
    "URL": "string"
  }
}
