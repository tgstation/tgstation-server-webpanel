        "postcss": "^8.2.15",
        "postcss-loader": "^5.3.0",
        "prettier": "^2.2.1",
        "react-dom": "^17.0.1",
        "react-refresh": "^0.10.0",
        "sass": "^1.32.13",
        "source-map-loader": "^2.0.2",
